        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wangyulong.user.mapper.UserMapper">
    <select id="checkUsername" resultType="com.wangyulong.user.vo.UserInfoVO">
        select user_name,password from user_info where user_name = #{userName}
